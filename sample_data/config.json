[
    {
        "re": "/path\\?arg=val",
        "path": "sample_data/body_1.json",
        "headers": {
            "Connection": "keep-alive"
        }
    },
    {
        "re": "/wrong$",
        "path": "/dev/null",
        "code": 400
    },
    {
        "re": "/stream$",
        "path": "sample_data/stream.data",
        "headers": {
            "Transfer-Encoding": "chunked",
            "Content-Type": "text/CSV; charset=utf-8"
        },
        "stream": true
    }
]